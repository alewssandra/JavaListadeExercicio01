package n4;

public class Main {

    public static void main(String[] args) {
        //Criação dos objetos.
        Empregado a = new Empregado("Fulano", "Silva", 2000.f);
        Empregado b = new Empregado("Ciclano", "Silva", 5000.f);

        //Utilização dos métodos.
        a.salarioAnual();
        a.setSalario(3000f);
        a.salarioAnual();

        b.salarioAnual();
    }

}
